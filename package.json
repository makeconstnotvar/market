    "private": true,
    "scripts": {
        "webpack-browser": "webpack --config webpack.browser.debug.js",
        "webpack-browser-watch": "webpack --config webpack.browser.debug.js --watch",
        "webpack-browser-release": "webpack --config webpack.browser.release.js",
        "webpack-server": "webpack --config webpack.server.js",
        "webpack-server-watch": "webpack --config webpack.server.js --watch",
        "gulp": "gulp --gulpfile gulpfile.client.js",
        "gulp-watch": "gulp watch --gulpfile gulpfile.client.js",
        "gulp-build-prod": "gulp build:prod --gulpfile gulpfile.prod.js",
        "gulp-build-test": "gulp build:test --gulpfile gulpfile.prod.js",
        "build-prod": "gulp clean:prod --gulpfile gulpfile.prod.js && npm run webpack-browser-release && npm run webpack-server && npm run gulp-build-prod",
        "build-test": "gulp clean:prod --gulpfile gulpfile.prod.js && npm run webpack-browser-release && npm run webpack-server && npm run gulp-build-test"
    },
